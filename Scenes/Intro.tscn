resource_name = "Intro"
script/source = "extends Node2D

onready var texts = $Camera/Texts

var appear_humans
var build_city

func _ready():
	seed(2)
	for human in $Phase2/Mover/Civilization.get_children():
		human.frame = randi()%4
		human.modulate.a = 0
	
	$Phase1.visible = true
	$Phase2.visible = false
	$Phase3.visible = false
	$Phase3a.visible = false
	
	recursive_invisible($Phase3/Buildings)
	
	return
	##DEBUGAŁKE
	$Phase1.visible = false
	$Phase3.visible = true
	$Phase3a.visible = true
	$Camera/Landscape.visible = true
	$Animator.play(\"Phase3\")
	##/ENDDEBUGAŁKE

func _process(delta):
	if appear_humans:
		$Phase2/Mover/Civilization.get_child(randi() % $Phase2/Mover/Civilization.get_child_count()).modulate.a += delta*10
	
	if build_city: recursive_visible($Phase3/Buildings)

func _on_phase_finished(phase):
	match phase:
		\"Phase1\":
			$Phase1.visible = false
			$Phase2.visible = true
			$Animator.play(\"Phase2\")
			
		\"Phase2\":
			$Phase3.visible = true
			$Camera/Landscape.visible = true
			$Animator.play(\"Phase3\")
			
		\"Phase3\":
			$Phase3/StarPower.texture = preload(\"res://Sprites/Intro/EvilStar.png\")
			$Animator.play(\"Phase4\")
			$Phase3a/Engineer/AnimationPlayer.play(\"Walk\")
			$Phase3a/Engineer2/AnimationPlayer.play(\"Walk\")
			$Phase3a/Engineer3/AnimationPlayer.play(\"Walk\")
			$Phase3a/Engineer4/AnimationPlayer.play(\"Walk\")
			recursive_retexture($Phase3/Buildings)

func animate_text(text):
	texts.get_node(\"TextAnimator\").get_animation(\"AnimateText\").track_set_path(0, text + \":percent_visible\")
	texts.get_node(\"TextAnimator\").get_animation(\"AnimateText\").track_set_path(1, text + \":modulate\")
	texts.get_node(\"TextAnimator\").play(\"AnimateText\")

func appear_humans():
	appear_humans = true

func build_city():
	build_city = true

func kill_dinosaurs():
	$Phase1/Sky/Dinosaur.texture = preload(\"res://Sprites/Intro/Deadosaur.png\")
	$Phase1/Sky/Dinosaur2.texture = preload(\"res://Sprites/Intro/Deadosaur.png\")
	$Phase1/Sky/Dinosaur3.texture = preload(\"res://Sprites/Intro/Deadosaur.png\")
	$Phase1/Sky/Dinosaur4.texture = preload(\"res://Sprites/Intro/Deadosaur.png\")

func world_burns():
	$Phase3/Creators.texture = preload(\"res://Sprites/Intro/Panic.png\")
	$Phase3/Creators2.texture = preload(\"res://Sprites/Intro/Panic.png\")
	$Phase3/Creators3.texture = preload(\"res://Sprites/Intro/Panic.png\")
	redeyize($Phase3a/Engineer)
	redeyize($Phase3a/Engineer2)
	redeyize($Phase3a/Engineer3)
	redeyize($Phase3a/Engineer4)
	redeyize($Phase3a/Engineer4)
	redeyize($Phase3a/Engineer4)
	redeyize($Phase3a/Engineer4)
	$Phase3a/Eyeflash.visible = true
	$Phase3a/Eyeflash2.visible = true

func redeyize(node):
	node.texture = preload(\"res://Sprites/Intro/Redeye.png\")
	node.hframes = 3
	
func recursive_invisible(node):
	node.self_modulate.a = 0
	for node2 in node.get_children(): recursive_invisible(node2)

func recursive_visible(node):
	if node.self_modulate.a < 1:
		node.self_modulate.a += 0.02
	else:
		for node2 in node.get_children(): recursive_visible(node2)
	
func recursive_retexture(node):
	if node is Sprite: node.texture = preload(\"res://Sprites/Intro/BuildingRuined.png\")
	for node2 in node.get_children(): recursive_retexture(node2)"
